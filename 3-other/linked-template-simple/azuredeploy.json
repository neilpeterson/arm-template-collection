{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {},
    "variables": {},
    "resources": [
        {
            "name": "vote-database",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/neilpeterson/arm-template-collection/master/linked-template/azuredeploy-cosmos.json",
                    "contentVersion": "1.0.0.0"
                }
            }
        },
        {
            "name": "vote-frontend",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2017-05-10",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'vote-database')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "https://raw.githubusercontent.com/neilpeterson/arm-template-collection/master/linked-template/azuredeploy-aci.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "cosmosDbKey": {
                        "value": "[reference('vote-database').outputs.dbkey.value]"
                    },
                    "cosmosDb": {
                        "value": "[reference('vote-database').outputs.dbendpoint.value]"
                    }
                }
            }
        }
    ],
    "outputs": {
        "file": {
           "type": "string",
           "value": "deployment().properties.template"
        }
    }
}